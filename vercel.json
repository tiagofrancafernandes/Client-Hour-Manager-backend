{
    "outputDirectory": "public",
    "installCommand": "echo 1 || true",
    "buildCommand": "echo 1 || true",
    "functions": {
        "api/*.php": {
            "runtime": "vercel-php@0.7.4"
        }
    },
    "rewrites": [
        {
            "source": "/favicon.ico",
            "destination": "https://coming-soon-page-in-vue-js.vercel.app/favicon.ico"
        },
        {
            "source": "/(.*)",
            "destination": "/api/index.php"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/api/index.php"
        }
    ],
    "env": {
        "APP_NAME": "Vercel Laravel",
        "APP_ENV": "production",
        "APP_DEBUG": "true",
        "APP_URL": "https://app-laravel-on-vercel.vercel.app",
        "VERCEL_DEMO_MODE": "true",
        "ON_SERVERLESS": "true",
        "CACHE_DRIVER": "array",
        "LOG_CHANNEL": "stderr",
        "SESSION_DRIVER": "array",
        "FILESYSTEM_DISK": "serverless_public",
        "LARAVEL_STORAGE_PATH": "/tmp/storage",
        "APP_CONFIG_CACHE": "/tmp/config.php",
        "APP_EVENTS_CACHE": "/tmp/events.php",
        "APP_PACKAGES_CACHE": "/tmp/packages.php",
        "APP_ROUTES_CACHE": "/tmp/routes.php",
        "APP_SERVICES_CACHE": "/tmp/services.php",
        "VIEW_COMPILED_PATH": "/tmp/views",
        "SSR_TEMP_PATH": "/tmp/ssr",
        "NODE_PATH": "node"
    }
}
